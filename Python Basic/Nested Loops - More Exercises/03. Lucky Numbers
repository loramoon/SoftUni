n = int(input())

for n1 in range (1, 10):
    for n2 in range (1, 10):
        for n3 in range (1, 10):
            for n4 in range (1, 10):
                if (n1+n2) != (n3+n4):
                    continue
                else:
                    numbers = 1000*n1 + 100*n2 + 10*n3 + n4
                    if n % (n1+n2) ==0:
                        print(numbers, end=' ')
