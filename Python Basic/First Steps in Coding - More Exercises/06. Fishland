price_skum = float(input())
price_caca = float(input())
price_palam = price_skum * 1.6
price_safr = price_caca * 1.8
price_seashell = 7.50
kg_palam = float(input())
kg_safr = float(input())
kg_seashell = int(input())
total = (price_palam * kg_palam) + (price_safr * kg_safr) + (price_seashell * kg_seashell)
print(f"{total:.2f}")
